rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2023, 5, 3);
    }
    match /travels/{travel} {
      allow write: if request.resource.data.userId == request.auth.uid;
      allow read: if (request.resource.data.isPublic == true) || (request.resource.data.userId == request.auth.uid);
    }
  }
}